import bcrypt from "bcryptjs";

const hashedPassword = "$2a$10$Me.aJdgDQqDY8tVp5vWl4.cuDxxpMXl78Uzblg/t5YsyIhstf/eNy"; // Stored hash
const enteredPassword = "NewSecurePassword123"; // The password you want to check

bcrypt.compare(enteredPassword, hashedPassword, (err, isMatch) => {
    if (err) {
        console.error(err);
    } else if (isMatch) {
        console.log("Password matches!");
    } else {
        console.log("Incorrect password.");
    }
});
